{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 1,
    "react/state-in-constructor": [
      "never"
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    ]
  }
}